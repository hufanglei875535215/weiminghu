select * from weiminghu;
select * from weiminghu;
CREATE TABLE IF NOT  EXISTS weiminghu(
    name STRING COMMENT "this is the name ",
    id BIGINT COMMENT "this is the id of the number",
    name_id BIGINT COMMENT "complex of main id"
    )
    ROW FORMAT DELIMITED
    FIELDS TERMINATED BY "\t"
    STORED AS TEXTFILE;
LOAD DATA LOCAL INPATH "/opt/weiminghu/text.txt" OVERWRITE INTO TABLE weiminghu;

    